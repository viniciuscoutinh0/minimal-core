{
    "name": "viniciuscoutinh0/minimal-core",
    "description": "A minimalistic PHP framework designed for simplicity and small-scale projects",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "spatie/ignition": "^1.15",
        "symfony/cache": "^7.3",
        "predis/predis": "^3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3",
        "pestphp/pest": "^3.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Viniciuscoutinh0\\Minimal\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Vinicius Coutinho",
            "email": "vinicius@promofarma.com.br"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
